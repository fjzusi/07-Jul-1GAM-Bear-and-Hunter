        <arguments>
          <argument>
            <kind>1</kind>
            <string>var nextAnimation = 0;

if(facing) {//Facing Right
    if(inAir) {//Jumping
        nextAnimation = 2;
    } else  if(walking) {//Walking
        nextAnimation = 1;
    } else {//Standing
        nextAnimation = 0;
    }
} else {//Facing Left
    if(inAir) {//Jumping
        nextAnimation = 5;
    } else if(walking) {//Walking
        nextAnimation = 4;
    } else {//Standing
        nextAnimation = 3;
    }
}

if(curAnimation != nextAnimation) {
    curAnimation = nextAnimation;
    image_index = 0;
    switch(nextAnimation) {
        case 0://Standing Right
            sprite_index = spr_Hunter_StandR;
            image_speed = 1;
            break;
        case 1://Walking Right
            sprite_index = spr_Hunter_WalkR;
            image_speed = 0.2;
            break;
        case 2://Jumping Right
            sprite_index = spr_Hunter_JumpR;
            image_speed = 0.1;
            break;
        case 3://Standing Left
            sprite_index = spr_Hunter_StandL;
            image_speed = 1;
            break;
        case 4://Walking Left
            sprite_index = spr_Hunter_WalkL;
            image_speed = 0.2;
            break;
        case 5://Jumping Left
            sprite_index = spr_Hunter_JumpL;
            image_speed = 0.1;
            break;
    }
}

if((sprite_index == spr_Hunter_JumpR || sprite_index == spr_Hunter_JumpL) &amp;&amp; image_index == 2) {
    image_speed = 0;
}
</string>
